# Homebrew
alias bi="brew install"
alias bic="brew install --cask --no-quarantine"
alias bl="brew list"
alias blc="brew list --cask"
alias bo="brew outdated"
alias bu="brew uninstall"
alias bup="brew update"

# Cloudconvert
alias "clc"="cloudconvert convert -f"
alias "clm"="cloudconvert merge"
alias "clo"="cloudconvert optimize --overwrite"

# Config files
alias "zshalias"="code $CONFIG/.zshalias"
alias "zshenv"="code $CONFIG/.zshenv"
alias "zshrc"="code $CONFIG/.zshrc"

# Filesystem
alias ..='cd ..'
alias ...='cd ../..'
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Git
alias "ga"="git add"
alias "ga*"="git add --all"
alias "gadog"="git log --all --decorate --oneline --graph"
alias "gc"="git checkout"
alias "gcm"="git commit -m"
alias "gcmi"="git commit -m 'Initial commit'"
alias "gi"="git init"
alias "gi*"="gi; ga*; gcmi"
alias "gp"="git push"
alias "gpsom"="git push --set-upstream origin main"

# Miscellaneous
alias "clear"="clear; startup_printout"
alias "py"="python3"
alias "python3"="conda_initialization; python3"
alias "vim"="nvim"
alias "wipe"="clear; cd ~/Desktop"
alias "ytdl"="youtube-dl"
